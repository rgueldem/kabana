{{#if group_name}}
  <span class="nav nav-pills">
    <li class="active"><a href="#apps/kabana/groups/{{group_id}}">{{group_name}}</a></li>
  </span>
{{/if}}

<div class="minimap">
  <div class="row-fluid">
    {{#each statuses}}
      <div class="span3">
        <div class="title">
          {{title}}
        </div>
      </div>
    {{/each}}
  </div>
  <div class="row-fluid">
    {{#each statuses}}
      <div class="span3 tickets">
        {{#each tickets}}
          <div class="ticketwrap {{#if active}}active{{/if}}" style="height:{{../../ticketHeight}}%">
            <div class="miniticket {{type}}"></div>
          </div>
        {{/each}}
      </div>
    {{/each}}
  </div>
</div>
