<div class="groups">
  {{#each groups}}
    {{name}}
  {{/each}}
</div>

<div class="tickets dropzone" data-status="open">
  {{#each tickets}}
    <div class="ticket" draggable="true" data-id="{{id}}">
      {{subject}}
    </div>
  {{/each}}
</div>

<div class="tickets dropzone" data-status="pending">
</div>
